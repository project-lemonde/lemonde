{
    "name": "Node.js 22",
    "dockerComposeFile": "compose.yaml",
    "service": "workspace",
    "workspaceFolder": "/workspace",
    "shutdownAction": "stopCompose",
    "customizations": {
        "vscode": {
            "extensions": [
                "biomejs.biome",
                "EditorConfig.EditorConfig",
                "GitHub.copilot",
                "GitHub.copilot-chat",
                "GitHub.vscode-pull-request-github",
                "eamodio.gitlens",
                "nrwl.angular-console"
            ]
        }
    }
}
