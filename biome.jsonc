{
    "$schema": "https://biomejs.dev/schemas/1.7.2/schema.json",
    "organizeImports": {
        "enabled": true
    },
    "formatter": {
        "enabled": true,
        "indentSize": 4,
        "indentStyle": "space",
        "indentWidth": 4,
        "lineWidth": 160,
        "lineEnding": "lf",
        "attributePosition": "auto"
    },
    "linter": {
        "enabled": true,
        "rules": {
            "all": true
        }
    }
}